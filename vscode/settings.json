  "php.validate.enable": false,
  "php.suggest.basic": false,
  "editor.quickSuggestions": {
    "comments": true,
    "strings": true
   },
   "better-comments.multilineComments": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Sections",
        "scope": "entity.name.section",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "Operatores",
        "scope": "keyword.operator.logical.php",
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "Functions",
        "scope": [
          "entity.name.function",
          "meta.require",
          "support.function.any-method",
          "variable.function"
        ],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "Classes",
        "scope": ["entity.name.type.namespace", "entity.name.type.class"],
        "settings": {
          "fontStyle": "bold"
        }
      }
    ]
  },
